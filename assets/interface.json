            "option": [
                "购买物品"
            ],
            "periodic": 1,
            "daily_start": 5
        },
        {
            "name": "领取体力",
            "entry": "体力",
            "periodic": 1,
            "daily_start": 5
        },
        {
            "name": "升级枪械",
